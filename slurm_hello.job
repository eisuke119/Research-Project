#!/bin/bash

#SBATCH -J hello
#SBATCH -p normal
#SBATCH -c 1                      # one CPU core
#SBATCH --array=1-2
#SBATCH -t 00:10:00
#SBATCH -o hello-%j.out
#SBATCH --mail-type=ALL
#SBATCH --mail-user=email
#SBATCH --partition=brown

# Load software
module load Anaconda3

# Run python script
srun python hello.py $SLURM_ARRAY_TASK_ID
